    "@vscode/test-electron": "2.3.6",
    "adm-zip": "0.5.10",
    "axios": "1.6.0",
    "buffer": "6.0.3",
    "constants-browserify": "1.0.0",
    "exponential-backoff": "3.1.1",
    "graceful-fs": "4.2.11",
    "js-md5": "0.8.3",
    "lodash": "4.17.21",
    "net": "1.0.2",
    "node-rsa": "1.1.1",
    "p-retry": "6.1.0",
    "process": "0.11.10",
    "retry": "0.13.1",
    "safe-buffer": "5.2.1",
    "stream-browserify": "3.0.0",
    "tsc": "2.0.4",
    "url": "0.11.3",
    "uuid": "9.0.1",
    "vscode-uri": "3.0.8"
  },
  "devDependencies": {
