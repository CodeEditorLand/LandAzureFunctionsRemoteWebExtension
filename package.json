    "typescript": "5.2.2",
    "typescript-esbuild": "0.3.1",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4"
  },
  "engines": {
    "vscode": "1.67.0"
  },
  "publishConfig": {
    "access": "public"
  }
